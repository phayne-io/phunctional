{
  "name": "phayne-io/phunctional",
  "description": "Phunctional is a little library that tries to bring to PHP some aspects of functional programing with util high order functions and functions for manage iterables.",
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Julien Guittard",
      "email": "julien.guittard@me.com"
    }
  ],
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "dg/bypass-finals": "^1.4",
    "malukenho/docheader": "^1.0",
    "php-coveralls/php-coveralls": "^v2.5",
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.7",
    "vimeo/psalm": "^4.30"
  },
  "autoload": {
    "files": [
      "src/_bootstrap.php"
    ]
  },
  "scripts": {
    "test-html": "XDEBUG_MODE=coverage phpunit --colors=always -vvv --coverage-html build/html"
  },
  "support": {
    "source": "https://github.com/phayne-io/phunctional",
    "issues": "https://github.com/phayne-io/phunctional/issues",
    "rss": "https://github.com/phayne-io/phunctional/releases.atom",
    "wiki": "https://github.com/phayne-io/phunctional/wiki"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  }
}